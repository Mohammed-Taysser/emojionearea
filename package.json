{
  "name": "emojionearea",
  "version": "4.0.0-alpha.0",
  "description": "EmojioneArea is an emojis picker and WYSIWYG-like text editor with EmojiOne/JoyPixels emojis support",
  "main": "dist/emojionearea.js",
  "scripts": {
    "build": "npm run build-main-prod",
    "build-dev": "npm run build-main-dev",
    "compile-css": "grunt prod",
    "build-main-prod": "cross-env NODE_ENV=production webpack --optimize-minimize -p --env.NODE_ENV=production --config webpack/main.config.js --progress",
    "build-main-dev": "cross-env NODE_ENV=development webpack --env.NODE_ENV=development --config webpack/main.config.js --display-error-details --progress",
    "test": "jest",
    "lint": "run-p lint:*",
    "friendly-lint:es": "eslint src --config=.eslintrc.js --cache --ext md,js,ts,jsx,tsx -f friendly",
    "lint:es": "eslint src --config=.eslintrc.js --cache --ext md,js,ts,jsx,tsx",
    "lint:tsc": "tsc --incremental false --noEmit"
  },
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mervick/emojionearea.git"
  },
  "keywords": [
    "emoji",
    "emojione",
    "joypixels",
    "picker",
    "emojionearea"
  ],
  "author": "Andrey Izman <izmanw@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mervick/emojionearea/issues"
  },
  "homepage": "https://github.com/mervick/emojionearea",
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/emojionearea"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "grunt": "^1.3.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-run": "^0.8.1",
    "grunt-sass": "^3.1.0",
    "jest": "^26.6.3",
    "load-grunt-tasks": "^5.1.0",
    "mocha": "^8.2.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "standard": "^16.0.3",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0"
  }
}
